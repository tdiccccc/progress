<%= form_with model: [:admin,genre], local: false do |f| %>
  <div class="row my-4 form-group">
    <%= f.label "ジャンル名", class: "col-sm-2 col-form-label" %>
    <div class="col-sm-6">
    <%= f.text_field :name, class: "form-control" %>
    </div>
    <%= f.submit "新規登録", class: "btn btn-success" %>
  </div>
<%end%>